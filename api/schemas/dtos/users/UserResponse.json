{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "required": [
        "id",
        "email",
        "resource_location"
    ],
    "properties": {
        "id": {
            "type": "string"
        },
        "email": {
            "type": "string"
        },
        "name": {
            "type": "string"
        },
        "resource_location": {
            "type": "string",
            "format": "url"
        }
    }
}